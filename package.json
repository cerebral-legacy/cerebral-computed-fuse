{
  "name": "cerebral-module-fuse",
  "version": "0.2.0",
  "description": "A cerebral module that adds fuzzy search to data in store",
  "main": "lib/index.js",
  "files": [
    "lib"
  ],
  "scripts": {
    "lint": "standard src/**/*.js test/**/*.js --verbose | snazzy",
    "start": "parallelshell 'babel src --watch --out-dir lib' 'watch --wait=1 \"npm run lint && mocha --compilers js:babel-core/register --recursive\" src test'",
    "prebuild": "npm run lint",
    "build": "rimraf lib && babel src --out-dir lib",
    "pretest": "npm run lint",
    "test": "mocha --compilers js:babel-core/register --recursive",
    "prepublish": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cerebral/cerebral-module-fuse.git"
  },
  "keywords": [
    "cerebral",
    "cerebral-module",
    "search",
    "fuzzy",
    "fuse"
  ],
  "author": "Garth Williams",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cerebral/cerebral-module-fuse/issues"
  },
  "homepage": "https://github.com/cerebral/cerebral-module-fuse#readme",
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-cli": "^6.11.4",
    "babel-core": "^6.11.4",
    "babel-preset-es2015": "^6.9.0",
    "cerebral": "^0.35.7",
    "cerebral-model-baobab": "^0.4.7",
    "cerebral-testable": "^0.3.2",
    "chai": "^3.5.0",
    "mocha": "^3.0.0",
    "parallelshell": "^2.0.0",
    "rimraf": "^2.5.4",
    "snazzy": "^4.0.0",
    "standard": "^7.1.2",
    "watch": "^0.19.1"
  },
  "dependencies": {
    "fuse.js": "^2.3.0"
  },
  "peerDependencies": {
    "cerebral": "^0.35.0"
  }
}
